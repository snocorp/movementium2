{
  "name": "movementium",
  "private": true,
  "version": "2.0.0",
  "description": "Pace calculator",
  "main": "",
  "scripts": {
    "clean": "rimraf dist/*",
    "copy": "copyfiles -f ./src/index.html ./src/browserconfig.xml ./src/manifest.json ./src/favicon.ico ./src/safari-pinned-tab.svg ./src/*.png ./dist",
    "dist": "npm run copy & webpack --env=dist",
    "lint": "eslint ./src",
    "posttest": "npm run lint",
    "serve": "node server.js --env=dev",
    "serve:dist": "node server.js --env=dist",
    "start": "node server.js --env=dev",
    "test": "karma start",
    "test:watch": "karma start --autoWatch=true --singleRun=false"
  },
